<xs:schema id="LoginsTable"
	targetNamespace="http://dev.stormraidergames.com/TortusLogins.xsd"
	elementFormDefault="qualified"
	xmlns="http://dev.stormraidergames.com/TortusLogins.xsd"
	xmlns:mstns="http://dev.stormraidergames.com/TortusLogins.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"		
>
	<xs:element name="Logins"> 
		<xs:complexType> 
			<xs:sequence> 
				<xs:element name="ID" type="xs:int" msdata:AutoIncrement="true" /> 
				<xs:element name="Login" type="xs:string" /> 
				<xs:element name="Password" type="xs:string" /> 
				<xs:element name="AccessLevel" type="xs:string" /> 
			</xs:sequence> 
		</xs:complexType> 
			<xs:key name="ID" msdata:PrimaryKey="true"> 
			<xs:selector xpath="." /> 
			<xs:field xpath="mstns:ID" /> 
		</xs:key> 
	</xs:element> 
</xs:schema>